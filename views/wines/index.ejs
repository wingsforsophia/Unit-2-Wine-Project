<%- include('../partials/header') %>
 

  <h4 class="display-4 ml-5" ><%= title %> - Click for Details </h4>

  <% wines.forEach((wine)=> { %>

    <div class="list-group">
      <a href="/wines/<%= wine._id %>" class="list-group-item list-group-item-action">
        <%= wine.name %> - <%= wine.varietal %>  <% if (wine.year != null) { %>  - <%= wine.year %> <% } %> 
      </a>
    </div>
    <% }) %>

      <%- include('../partials/footer') %>